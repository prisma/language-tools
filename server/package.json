{
  "name": "@prisma/language-server",
  "description": "Implementation of a language server in node.",
  "version": "0.0.40",
  "author": "Prisma",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10"
  },
  "prisma": {
    "version": "052a692dc18f79e7b60004a3b29f8a15d53e5f3c"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/prisma/vscode"
  },
  "main": "./out/index.js",
  "dependencies": {
    "prismafile": "1.0.3",
    "@prisma/fetch-engine": "2.0.0-alpha.1182",
    "@prisma/get-platform": "2.0.0-alpha.1182",
    "@prisma/sdk": "2.0.0-alpha.1182",
    "pkg-dir": "4.2.0",
    "vscode-languageserver": "6.1.1",
    "vscode-languageserver-textdocument": "1.0.1",
    "vscode-uri": "2.1.1",
    "prettier": "2.0.5",
    "lint-staged": "10.1.2",
    "husky": "4.2.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-prettier": "3.1.2",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "@typescript-eslint/parser": "2.27.0"
  },
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint  --fix --ext .js,.ts ."
  },
  "files": [
    "out"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "autocomplete",
    "intellisense",
    "language",
    "prisma",
    "server",
    "formatting",
    "linting"
  ],
  "lint-staged": {
    "*/**/*.{js,ts,css,json,md}": [
      "prettier --write",
      "git add"
    ],
    "*/**/*.{js,ts}": [
      "eslint --fix --ext .js,.ts ."
    ]
  }
}
