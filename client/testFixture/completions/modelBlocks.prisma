model User {
    firstName String
    lastName String
    email String @unique
    isAdmin Boolean @default(false)


}


model Post {
    id Int @id @default()
    email String? @unique
    name String

}

model Person {
    id String 
    name Post 
}

model Test {
  email    String  @unique
  isAdmin  Boolean @default()
}

model Cat {
    id String @id @default()
    name String
    createdAt  DateTime    @default()
}