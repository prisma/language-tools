{
  "name": "prisma-insider",
  "displayName": "Prisma - Insider",
  "description": "This is the Insider Build of the Prisma VSCode extension (only use it if you are also using the dev version of the CLI.",
  "version": "9.0.98",
  "license": "Apache-2.0",
  "icon": "logo_white.png",
  "engines": {
    "vscode": "^1.49.0"
  },
  "publisher": "Prisma",
  "categories": [
    "Programming Languages"
  ],
  "dependencies": {
    "checkpoint-client": "1.1.13",
    "crypto": "1.0.1",
    "chokidar": "3.4.2",
    "@prisma/language-server": "9.0.98",
    "vscode-languageclient": "6.1.3"
  },
  "optionalDependencies": {
    "fsevents": "2.1.3"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/prisma/vscode"
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "npm run build -- -w",
    "vscode:prepublish": "cd .. && cd .. && tsc --build --clean && npm run compile",
    "package": "vsce package"
  },
  "main": "./dist/src/extension.js",
  "activationEvents": [
    "onLanguage:prisma"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prisma",
        "aliases": [
          "Prisma",
          "prisma"
        ],
        "extensions": [
          ".prisma"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prisma",
        "scopeName": "source.prisma",
        "path": "./syntaxes/prisma.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.js",
          "source.ts",
          "source.js.jsx",
          "source.tsx",
          "source.vue"
        ],
        "scopeName": "inline.prisma",
        "path": "./syntaxes/prisma.js.json",
        "embeddedLanguages": {
          "meta.embedded.block.prisma": "prisma"
        }
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Language server configuration",
      "properties": {
        "prismaLanguageServer.prismaFmtBinPath": {
          "scope": "resource",
          "type": "string",
          "description": "Option to set the path to the prisma-fmt binary."
        }
      }
    },
    "commands": [
      {
        "command": "prisma.restartLanguageServer",
        "title": "Prisma: Restart Language Server"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "7.1.3",
    "@types/vscode": "1.49.0",
    "vsce": "1.81.1",
    "vscode-test": "1.4.0",
    "@types/mocha": "8.0.3",
    "mocha": "8.1.3",
    "semver": "7.3.2",
    "typescript": "4.0.3",
    "ovsx": "0.1.0-next.e000fdb"
  },
  "gitHead": "7d51b157647fe1705813a30d1a77b8ccf136b8d4",
  "publishConfig": {
    "access": "public"
  },
  "preview": true
}
