{
  "name": "prisma-insider",
  "displayName": "Prisma - Insider",
  "description": "This is the Insider Build of the Prisma VSCode extension (only use it if you are also using the  version of the CLI.",
  "version": "2.0.1",
  "license": "Apache-2.0",
  "icon": "logo_white.png",
  "engines": {
    "vscode": "^1.43.0"
  },
  "publisher": "Prisma",
  "categories": [
    "Programming Languages"
  ],
  "dependencies": {
    "@prisma/language-server": "2.0.1",
    "vscode-languageclient": "6.1.3"
  },
  "repository": {
    "type": "github",
    "url": "https://github.com/prisma/vscode"
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "npm run build -- -w",
    "vscode:prepublish": "npm run build",
    "vsce:publish": "sh scripts/publish.sh",
    "vsce:check": "yarn shell:lint && sh scripts/check-update.sh",
    "shell:lint": "shellcheck -x scripts/*.sh",
    "test": "sh ./scripts/e2e.sh"
  },
  "main": "./dist/src/extension.js",
  "activationEvents": [
    "onLanguage:prisma"
  ],
  "contributes": {
    "languages": [
      {
        "id": "prisma",
        "aliases": [
          "Prisma",
          "prisma"
        ],
        "extensions": [
          ".prisma"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "prisma",
        "scopeName": "source.prisma",
        "path": "./syntaxes/prisma.tmLanguage.json"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Language server configuration",
      "properties": {
        "languageServerExample.maxNumberOfProblems": {
          "scope": "resource",
          "type": "number",
          "default": 100,
          "description": "Controls the maximum number of problems produced by the server."
        },
        "languageServerExample.trace.server": {
          "scope": "window",
          "type": "string",
          "enum": [
            "off",
            "messages",
            "verbose"
          ],
          "default": "off",
          "description": "Traces the communication between VS Code and the language server."
        }
      }
    },
    "commands": [
      {
        "command": "prisma.restartLanguageServer",
        "title": "Prisma: Restart Language Server"
      }
    ]
  },
  "devDependencies": {
    "@types/glob": "5.0.35",
    "@types/vscode": "1.43.0",
    "vsce": "1.75.0",
    "vscode-test": "1.3.0",
    "yarn": "1.22.4",
    "@types/mocha": "5.2.7",
    "mocha": "7.1.1",
    "semver": "7.3.2",
    "typescript": "3.8.3"
  },
  "gitHead": "7d51b157647fe1705813a30d1a77b8ccf136b8d4",
  "publishConfig": {
    "access": "public"
  },
  "preview": true
}
