generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["views"]
}

datasource db {
    provider = "postgresql"

}

view Window {
    id   Int    @id
    Prop Prop[]
}

model Prop {
    id       Int    @id
    item     Window @relation(fields: [windowId], references: [id])
    windowId Int
}
