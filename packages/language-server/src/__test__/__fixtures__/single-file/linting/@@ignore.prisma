datasource db {
    provider = "sqlite"

}

generator client {
    provider = "prisma-client-js"
}

model Post {
    id        Int     @id @default(autoincrement())
    title     String
    content   String?
    published Boolean @default(false)
    authorId  Int?

    @@ignore
}

model User {
    id    Int     @id @default(autoincrement())
    email String  @unique
    name  String?
}
